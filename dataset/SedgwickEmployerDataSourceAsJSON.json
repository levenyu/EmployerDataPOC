{
	"name": "SedgwickEmployerDataSourceAsJSON",
	"properties": {
		"description": "Sedgwick employer data source as JSON file in the employer BLOB container in the inputJSON folder.",
		"linkedServiceName": {
			"referenceName": "EmployerDataSource",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"SourceFileName": {
				"type": "String",
				"defaultValue": "SedgwickClaimListExample.json"
			},
			"SourceFolderName": {
				"type": "String",
				"defaultValue": "employer/inputJSON"
			},
			"Structure": {
				"defaultValue": "[{\"name\":\"Name\",\"type\":\"String\",\"description\":\"Employer Name\"},{\"name\":\"FEIN\",\"type\":\"String\",\"description\":\"Employer FEIN\"},{\"name\":\"UniqueId\",\"type\":\"String\",\"description\":\"Employer Unique ID\"},{\"name\":\"AddrLine1\",\"type\":\"String\",\"description\":\"Address line 1\"},{\"name\":\"AddrLine2\",\"type\":\"String\",\"description\":\"Address line 2\"},{\"name\":\"City\",\"type\":\"String\",\"description\":\"City\"},{\"name\":\"State\",\"type\":\"String\",\"description\":\"State\"},{\"name\":\"Zip\",\"type\":\"String\",\"description\":\"Zip\"},{\"name\":\"Country\",\"type\":\"String\",\"description\":\"Country\"}]"
			}
		},
		"type": "AzureBlob",
		"structure": {
			"value": "@dataset().Structure",
			"type": "Expression"
		},
		"typeProperties": {
			"format": {
				"type": "JsonFormat",
				"filePattern": "setOfObjects",
				"jsonPathDefinition": {
					"Name": "$.['ClaimList']['Claim']['ClaimExpanded']['Content']['claimExpanded']['employer']['name']",
					"FEIN": "$.['ClaimList']['Claim']['ClaimExpanded']['Content']['claimExpanded']['employer']['FEIN']",
					"UniqueId": "$.['ClaimList']['Claim']['ClaimExpanded']['Content']['claimExpanded']['employer']['uniqueId']",
					"AddrLine1": "$.['ClaimList']['Claim']['ClaimExpanded']['Content']['claimExpanded']['employer']['address']['line1']",
					"AddrLine2": "$.['ClaimList']['Claim']['ClaimExpanded']['Content']['claimExpanded']['employer']['address']['line2']",
					"City": "$.['ClaimList']['Claim']['ClaimExpanded']['Content']['claimExpanded']['employer']['address']['city']",
					"State": "$.['ClaimList']['Claim']['ClaimExpanded']['Content']['claimExpanded']['employer']['address']['state']",
					"Zip": "$.['ClaimList']['Claim']['ClaimExpanded']['Content']['claimExpanded']['employer']['address']['zip']",
					"Country": "$.['ClaimList']['Claim']['ClaimExpanded']['Content']['claimExpanded']['employer']['address']['country']"
				}
			},
			"fileName": {
				"value": "@dataset().SourceFileName",
				"type": "Expression"
			},
			"folderPath": {
				"value": "@dataset().SourceFolderName",
				"type": "Expression"
			}
		}
	}
}