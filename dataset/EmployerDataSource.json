{
	"name": "EmployerDataSource",
	"properties": {
		"description": "Sedgwick employer data source as XML file in the Employer BLOB container",
		"linkedServiceName": {
			"referenceName": "EmployerDataSource",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"SourceFileName": {
				"type": "String",
				"defaultValue": "SedgwickClaimFile_201808260955.xml"
			},
			"SourceFolderName": {
				"type": "String",
				"defaultValue": "employer"
			},
			"Structure": {
				"type": "String",
				"defaultValue": [
					{
						"name": "Prop_0",
						"type": "String"
					},
					{
						"name": "Prop_1",
						"type": "String"
					}
				]
			}
		},
		"type": "AzureBlob",
		"structure": {
			"value": "@concat(dataset().SourceFolderName,'/',dataset().SourceFileName)",
			"type": "Expression"
		},
		"typeProperties": {
			"format": {
				"type": "TextFormat",
				"columnDelimiter": "\u0001",
				"rowDelimiter": "",
				"nullValue": "\\N",
				"treatEmptyAsNull": true,
				"skipLineCount": 0,
				"firstRowAsHeader": false
			},
			"fileName": {
				"value": "@dataset().SourceFileName",
				"type": "Expression"
			},
			"folderPath": {
				"value": "@dataset().SourceFolderName",
				"type": "Expression"
			}
		}
	}
}