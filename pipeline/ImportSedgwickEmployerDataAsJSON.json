{
	"name": "ImportSedgwickEmployerDataAsJSON",
	"properties": {
		"description": "Import Sedgwick Employer Data from XML data source file; convert it to JSON file in the Azure BLOB storage then import it to the PolarisEtl ASD.",
		"activities": [
			{
				"name": "CopyJSONToASD",
				"description": "Copy JSON To ASD",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "LookupMetadata",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"source": {
						"type": "BlobSource",
						"recursive": true
					},
					"sink": {
						"type": "SqlSink",
						"writeBatchSize": 10000,
						"sqlWriterStoredProcedureName": {
							"value": "@pipeline().parameters.SinkProcedureName",
							"type": "Expression"
						},
						"sqlWriterTableType": {
							"value": "@pipeline().parameters.SinkUserDefinedTableType",
							"type": "Expression"
						},
						"storedProcedureParameters": {
							"SourceFileName": {
								"type": "String",
								"value": {
									"value": "@pipeline().parameters.SourceFileName",
									"type": "Expression"
								}
							}
						}
					},
					"enableStaging": false,
					"dataIntegrationUnits": 0,
					"translator": {
						"value": "@pipeline().parameters.DataMappingStructure",
						"type": "Expression"
					}
				},
				"inputs": [
					{
						"referenceName": "SedgwickEmployerDataSourceAsJSON",
						"type": "DatasetReference",
						"parameters": {
							"SourceFileName": {
								"value": "@pipeline().parameters.SourceFileName",
								"type": "Expression"
							},
							"SourceFolderName": {
								"value": "@pipeline().parameters.SourceFolderName",
								"type": "Expression"
							},
							"Structure": {
								"value": "@pipeline().parameters.SourceDataStructure",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "PolarisEtlASD",
						"type": "DatasetReference",
						"parameters": {
							"Structure": {
								"value": "@pipeline().parameters.SinkDataStructure",
								"type": "Expression"
							},
							"TargetTableName": {
								"value": "@pipeline().parameters.TargetTableName",
								"type": "Expression"
							}
						}
					}
				]
			},
			{
				"name": "LookupMetadata",
				"type": "Lookup",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"source": {
						"type": "SqlSource",
						"sqlReaderQuery": "SELECT datafactoryloadid, \n       pipeline, \n       tabletype, \n\t   sourcedatabase,\n       sourcetable, \n\t   targetdatabase,\n       targetsink, \n       sortorder, \n       Isnull(directsourcequery, sourcequery) AS SourceQuery, \n       SourceStructure, \n       SourceStoredProcedure, \n       SourceStoredProcedureTableType, \n       TargetStructure, \n       targetprepsinkscript, \n       ColumnMapping, \n       isactive, \n       ispurge, \n       isincremental \nFROM   metadata.datafactoryload d \nWHERE  1=1 \n\t   and Pipeline = 'ImportSedgwickEmployerDataAsJSON'\norder by datafactoryloadid desc"
					},
					"dataset": {
						"referenceName": "PolarisEtlASD",
						"type": "DatasetReference",
						"parameters": {
							"Structure": [
								{
									"name": "EmployerName",
									"type": "String",
									"description": "Employer Name"
								},
								{
									"name": "EmployerFEIN",
									"type": "String",
									"description": "Employer FEIN"
								},
								{
									"name": "EmployerUniqueId",
									"type": "String",
									"description": "Employer Unique ID"
								},
								{
									"name": "EmployerAddressLine1",
									"type": "String",
									"description": "Address line 1"
								},
								{
									"name": "EmployerAddressLine2",
									"type": "String",
									"description": "Address line 2"
								},
								{
									"name": "EmployerCity",
									"type": "String",
									"description": "City"
								},
								{
									"name": "EmployerState",
									"type": "String",
									"description": "State"
								},
								{
									"name": "EmployerZip",
									"type": "String",
									"description": "Zip"
								},
								{
									"name": "EmployerCountry",
									"type": "String",
									"description": "Country"
								}
							],
							"TargetTableName": "ImportDataTable"
						}
					}
				}
			}
		],
		"parameters": {
			"SourceFileName": {
				"type": "String",
				"defaultValue": "SedgwickClaimListExample.json"
			},
			"SourceFolderName": {
				"type": "String",
				"defaultValue": "employer/inputJSON"
			},
			"SourceDataStructure": {
				"type": "Object",
				"defaultValue": [
					{
						"name": "name",
						"type": "String",
						"description": "Employer Name"
					},
					{
						"name": "FEIN",
						"type": "String",
						"description": "Employer FEIN"
					},
					{
						"name": "uniqueId",
						"type": "String",
						"description": "Employer Unique ID"
					},
					{
						"name": "line1",
						"type": "String",
						"description": "Address line 1"
					},
					{
						"name": "line2",
						"type": "String",
						"description": "Address line 2"
					},
					{
						"name": "city",
						"type": "String",
						"description": "City"
					},
					{
						"name": "state",
						"type": "String",
						"description": "State"
					},
					{
						"name": "zip",
						"type": "String",
						"description": "Zip"
					},
					{
						"name": "country",
						"type": "String",
						"description": "Country"
					}
				]
			},
			"SinkDataStructure": {
				"type": "Object",
				"defaultValue": [
					{
						"name": "EmployerName",
						"type": "String",
						"description": "Employer Name"
					},
					{
						"name": "EmployerFEIN",
						"type": "String",
						"description": "Employer FEIN"
					},
					{
						"name": "EmployerUniqueId",
						"type": "String",
						"description": "Employer Unique ID"
					},
					{
						"name": "EmployerAddressLine1",
						"type": "String",
						"description": "Address line 1"
					},
					{
						"name": "EmployerAddressLine2",
						"type": "String",
						"description": "Address line 2"
					},
					{
						"name": "EmployerCity",
						"type": "String",
						"description": "City"
					},
					{
						"name": "EmployerState",
						"type": "String",
						"description": "State"
					},
					{
						"name": "EmployerZip",
						"type": "String",
						"description": "Zip"
					},
					{
						"name": "EmployerCountry",
						"type": "String",
						"description": "Country"
					}
				]
			},
			"DataMappingStructure": {
				"type": "Object",
				"defaultValue": {
					"type": "TabularTranslator",
					"columnMappings": {
						"name": "EmployerName",
						"FEIN": "EmployerFEIN",
						"uniqueId": "EmployerUniqueId",
						"line1": "EmployerAddressLine1",
						"line2": "EmployerAddressLine2",
						"city": "EmployerCity",
						"state": "EmployerState",
						"zip": "EmployerZip",
						"country": "EmployerCountry"
					}
				}
			},
			"TargetTableName": {
				"type": "String",
				"defaultValue": "master.Employer"
			},
			"SinkProcedureName": {
				"type": "String",
				"defaultValue": "master.uspImportSedgwickEmployerData"
			},
			"SinkUserDefinedTableType": {
				"type": "String",
				"defaultValue": "master.udttSedgwickEmployerData"
			}
		}
	}
}