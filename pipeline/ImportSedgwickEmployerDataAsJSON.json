{
	"name": "ImportSedgwickEmployerDataAsJSON",
	"properties": {
		"description": "Import Sedgwick Employer Data from XML data source file, converted to JSON file, in the Azure BLOB storage to the PolarisEtl ASD.",
		"activities": [
			{
				"name": "CopyXmlToJSONToASD",
				"description": "Copy Xml To JSON To ASD",
				"type": "Copy",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "DataMappingStructure",
						"value": "{\"type\":\"TabularTranslator\",\"columnMappings\":{\"Prop_0\":\"XMLData\"}}"
					}
				],
				"typeProperties": {
					"source": {
						"type": "BlobSource",
						"recursive": true
					},
					"sink": {
						"type": "SqlSink",
						"writeBatchSize": 10000,
						"sqlWriterStoredProcedureName": "master.uspImportSedgwickClaimData",
						"sqlWriterTableType": "master.udttSedgwickClaimData",
						"storedProcedureParameters": {
							"SourceXmlFileName": {
								"type": "String",
								"value": {
									"value": "@pipeline().parameters.SourceFileName",
									"type": "Expression"
								}
							},
							"TestMode": {
								"type": "Byte",
								"value": "0"
							}
						}
					},
					"enableStaging": false,
					"dataIntegrationUnits": 0,
					"translator": {
						"value": "@pipeline().parameters.DataMappingStructure",
						"type": "Expression"
					}
				},
				"inputs": [
					{
						"referenceName": "SedgwickEmployerDataSource",
						"type": "DatasetReference",
						"parameters": {
							"SourceFileName": "SedgwickClaimFile.xml",
							"SourceFolderName": "employer/inputXML",
							"Structure": [
								{
									"name": "Prop_0",
									"type": "String"
								}
							]
						}
					}
				],
				"outputs": [
					{
						"referenceName": "PolarisEtlASD",
						"type": "DatasetReference",
						"parameters": {
							"Structure": {
								"value": "@pipeline().parameters.SinkDataStructure",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"SourceFileName": {
				"type": "String",
				"defaultValue": "SedgwickClaimFile.json"
			},
			"SourceFolderName": {
				"type": "String",
				"defaultValue": "employer/inputJSON"
			},
			"SourceDataStructure": {
				"type": "String",
				"defaultValue": [
					{
						"name": "Prop_0",
						"type": "String"
					}
				]
			},
			"SinkDataStructure": {
				"type": "String",
				"defaultValue": [
					{
						"name": "JSONData",
						"type": "String"
					}
				]
			},
			"DataMappingStructure": {
				"type": "String",
				"defaultValue": {
					"type": "TabularTranslator",
					"columnMappings": {
						"Prop_0": "JSONData"
					}
				}
			}
		}
	}
}