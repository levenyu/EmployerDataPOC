{
	"name": "ImportSedgwickEmployerDataAsXML",
	"properties": {
		"description": "Import Sedgwick Employer Data from XML data source file in the Azure BLOB storage to the PolarisEtl ASD.",
		"activities": [
			{
				"name": "CopyXmlToASD",
				"description": "Copy Xml To ASD",
				"type": "Copy",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"source": {
						"type": "BlobSource",
						"recursive": true
					},
					"sink": {
						"type": "SqlSink",
						"writeBatchSize": 10000,
						"sqlWriterStoredProcedureName": "SomeSPROC"
					},
					"enableStaging": false,
					"dataIntegrationUnits": 0,
					"translator": {
						"value": "@pipeline().parameters.DataMappingStructure",
						"type": "Expression"
					}
				},
				"inputs": [
					{
						"referenceName": "SedgwickEmployerDataSourceAsXML",
						"type": "DatasetReference",
						"parameters": {
							"SourceFileName": "SedgwickClaimFile.xml",
							"SourceFolderName": "employer/inputXML",
							"Structure": [
								{
									"name": "Prop_0",
									"type": "String"
								}
							]
						}
					}
				],
				"outputs": [
					{
						"referenceName": "PolarisEtlASD",
						"type": "DatasetReference",
						"parameters": {
							"Structure": [
								{
									"name": "EmployerName",
									"type": "String"
								},
								{
									"name": "EmployerFEIN",
									"type": "String"
								},
								{
									"name": "EmployerUniqueId",
									"type": "String"
								},
								{
									"name": "EmployerAddressLine1",
									"type": "String"
								},
								{
									"name": "EmployerAddressLine2",
									"type": "String"
								},
								{
									"name": "EmployerCity",
									"type": "String"
								},
								{
									"name": "EmployerState",
									"type": "String"
								},
								{
									"name": "EmployerZip",
									"type": "String"
								},
								{
									"name": "EmployerCountry",
									"type": "String"
								}
							]
						}
					}
				]
			}
		],
		"parameters": {
			"SourceFileName": {
				"type": "String",
				"defaultValue": "SedgwickClaimFile.xml"
			},
			"SourceFolderName": {
				"type": "String",
				"defaultValue": "employer/inputXML"
			},
			"SourceDataStructure": {
				"type": "Object",
				"defaultValue": [
					{
						"name": "Prop_0",
						"type": "String"
					}
				]
			},
			"SinkDataStructure": {
				"type": "Object",
				"defaultValue": [
					{
						"name": "XMLData",
						"type": "String"
					}
				]
			},
			"DataMappingStructure": {
				"type": "Object",
				"defaultValue": {
					"type": "TabularTranslator",
					"columnMappings": {
						"Prop_0": "XMLData"
					}
				}
			}
		}
	}
}