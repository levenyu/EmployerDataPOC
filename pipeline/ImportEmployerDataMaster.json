{
	"name": "ImportEmployerDataMaster",
	"properties": {
		"description": "Import Employer Data Master pipeline",
		"activities": [
			{
				"name": "LookupMetadata",
				"description": "Lookup metadata from the metadata.DataFactoryLoad table",
				"type": "Lookup",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"source": {
						"type": "SqlSource",
						"sqlReaderQuery": "select\n\td.DataFactoryLoadId,\n\td.Pipeline,\n\td.TargetDatabase,\n\td.TargetSink,\n\td.SortOrder,\n\td.SourceStructure,\n\td.TargetStructure,\n\td.ColumnMapping,\n\td.SourceStoredProcedure,\n\td.SourceStoredProcedureTableType\nfrom\n\tmetadata.DataFactoryLoad d\nwhere\n\t1 = 1\n\tand d.Pipeline = 'ImportSedgwickEmployerDataAsJSON'\norder by\n\td.DataFactoryLoadId desc"
					},
					"dataset": {
						"referenceName": "PolarisEtlASD",
						"type": "DatasetReference",
						"parameters": {
							"Structure": [
								{
									"name": "EmployerName",
									"type": "String",
									"description": "Employer Name"
								},
								{
									"name": "EmployerFEIN",
									"type": "String",
									"description": "Employer FEIN"
								},
								{
									"name": "EmployerUniqueId",
									"type": "String",
									"description": "Employer Unique ID"
								},
								{
									"name": "EmployerAddressLine1",
									"type": "String",
									"description": "Address line 1"
								},
								{
									"name": "EmployerAddressLine2",
									"type": "String",
									"description": "Address line 2"
								},
								{
									"name": "EmployerCity",
									"type": "String",
									"description": "City"
								},
								{
									"name": "EmployerState",
									"type": "String",
									"description": "State"
								},
								{
									"name": "EmployerZip",
									"type": "String",
									"description": "Zip"
								},
								{
									"name": "EmployerCountry",
									"type": "String",
									"description": "Country"
								}
							],
							"TargetTableName": "master.Employer"
						}
					}
				}
			},
			{
				"name": "ExecuteChildPipeline",
				"description": "Execute ImportSedgwickEmployerDataAsJSON Pipeline",
				"type": "ExecutePipeline",
				"dependsOn": [
					{
						"activity": "LookupMetadata",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"typeProperties": {
					"pipeline": {
						"referenceName": "ImportSedgwickEmployerDataAsJSON",
						"type": "PipelineReference"
					},
					"parameters": {
						"SourceFileName": {
							"value": "@pipeline().parameters.SourceFileName",
							"type": "Expression"
						},
						"SourceFolderName": {
							"value": "@pipeline().parameters.SourceFolderName",
							"type": "Expression"
						},
						"SourceDataStructure": "[{\"name\":\"name\",\"type\":\"String\",\"description\":\"Employer Name\"},{\"name\":\"FEIN\",\"type\":\"String\",\"description\":\"Employer FEIN\"},{\"name\":\"uniqueId\",\"type\":\"String\",\"description\":\"Employer Unique ID\"},{\"name\":\"line1\",\"type\":\"String\",\"description\":\"Address line 1\"},{\"name\":\"line2\",\"type\":\"String\",\"description\":\"Address line 2\"},{\"name\":\"city\",\"type\":\"String\",\"description\":\"City\"},{\"name\":\"state\",\"type\":\"String\",\"description\":\"State\"},{\"name\":\"zip\",\"type\":\"String\",\"description\":\"Zip\"},{\"name\":\"country\",\"type\":\"String\",\"description\":\"Country\"}]",
						"SinkDataStructure": "[{\"name\":\"EmployerName\",\"type\":\"String\",\"description\":\"Employer Name\"},{\"name\":\"EmployerFEIN\",\"type\":\"String\",\"description\":\"Employer FEIN\"},{\"name\":\"EmployerUniqueId\",\"type\":\"String\",\"description\":\"Employer Unique ID\"},{\"name\":\"EmployerAddressLine1\",\"type\":\"String\",\"description\":\"Address line 1\"},{\"name\":\"EmployerAddressLine2\",\"type\":\"String\",\"description\":\"Address line 2\"},{\"name\":\"EmployerCity\",\"type\":\"String\",\"description\":\"City\"},{\"name\":\"EmployerState\",\"type\":\"String\",\"description\":\"State\"},{\"name\":\"EmployerZip\",\"type\":\"String\",\"description\":\"Zip\"},{\"name\":\"EmployerCountry\",\"type\":\"String\",\"description\":\"Country\"}]",
						"DataMappingStructure": "{\"type\":\"TabularTranslator\",\"columnMappings\":{\"name\":\"EmployerName\",\"FEIN\":\"EmployerFEIN\",\"uniqueId\":\"EmployerUniqueId\",\"line1\":\"EmployerAddressLine1\",\"line2\":\"EmployerAddressLine2\",\"city\":\"EmployerCity\",\"state\":\"EmployerState\",\"zip\":\"EmployerZip\",\"country\":\"EmployerCountry\"}}",
						"TargetTableName": "ImportDataTable",
						"SinkProcedureName": "master.uspImportSedgwickEmployerData",
						"SinkUserDefinedTableType": "master.udttSedgwickEmployerData"
					}
				}
			}
		],
		"parameters": {
			"SourceFileName": {
				"type": "String",
				"defaultValue": "SedgwickClaimListExample.json"
			},
			"SourceFolderName": {
				"type": "String",
				"defaultValue": "employer/inputJSON"
			}
		}
	}
}