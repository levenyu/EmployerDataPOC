{
	"name": "ImportEmployerData",
	"properties": {
		"description": "Import Employer Data",
		"activities": [
			{
				"name": "CopyXmlToASD",
				"description": "Copy Xml To ASD",
				"type": "Copy",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"source": {
						"type": "BlobSource",
						"recursive": true
					},
					"sink": {
						"type": "SqlSink",
						"writeBatchSize": 10000
					},
					"enableStaging": false,
					"dataIntegrationUnits": 0
				},
				"inputs": [
					{
						"referenceName": "EmployerDataSource",
						"type": "DatasetReference",
						"parameters": {
							"SourceFileName": {
								"value": "@pipeline().parameters.SourceFileName",
								"type": "Expression"
							},
							"SourceFolderName": {
								"value": "@pipeline().parameters.SourceFolderName",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "PolarisEtlASD",
						"type": "DatasetReference",
						"parameters": {
							"SourceFileName": {
								"value": "@pipeline().parameters.SourceFileName",
								"type": "Expression"
							},
							"SourceFolderName": {
								"value": "@pipeline().parameters.SourceFolderName",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"SourceFileName": {
				"type": "String",
				"defaultValue": "SedgwickClaimFile_201808260955.xml"
			},
			"SourceFolderName": {
				"type": "String",
				"defaultValue": "employer"
			},
			"SourceDataStructure": {
				"type": "String",
				"defaultValue": [
					{
						"name": "Prop_0",
						"type": "String"
					},
					{
						"name": "Prop_1",
						"type": "String"
					}
				]
			},
			"SinkDataStructure": {
				"type": "String",
				"defaultValue": [
					{
						"name": "XMLData",
						"type": "String"
					},
					{
						"name": "SourceFileName",
						"type": "String"
					}
				]
			},
			"DataMappingStructure": {
				"type": "String",
				"defaultValue": {
					"type": "TabularTranslator",
					"columnMappings": {
						"Prop_0": "XMLData",
						"Prop_1": "SourceFileName"
					}
				}
			}
		}
	}
}