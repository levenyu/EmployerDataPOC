{
	"name": "ImportEmployerData",
	"properties": {
		"description": "Import Employer Data",
		"activities": [
			{
				"name": "CopyXmlToASD",
				"description": "Copy Xml To ASD",
				"type": "Copy",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"source": {
						"type": "BlobSource",
						"recursive": false
					},
					"sink": {
						"type": "SqlSink",
						"writeBatchSize": 10000
					},
					"enableStaging": false,
					"dataIntegrationUnits": 0
				},
				"inputs": [
					{
						"referenceName": "EmployerDataSource",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "PolarisEtlASD",
						"type": "DatasetReference"
					}
				]
			}
		],
		"parameters": {
			"SourceFileName": {
				"type": "String",
				"defaultValue": "SedgwickClaimFile_201808260955.xml"
			},
			"SourceFolderName": {
				"type": "String",
				"defaultValue": "employer"
			}
		}
	}
}